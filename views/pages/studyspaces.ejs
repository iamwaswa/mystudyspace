<% include ../partials/header %>
  <link type='text/css' rel='stylesheet' href='/stylesheets/studyspaces.css'>
</head>
<body>
  <main>
    <% include ../partials/navigation %>
    <section
      class='container'
    >
      <h2>
        Studyspaces
      </h2>
      <ul>
        <% studyspaces.map((studyspace) => { %>
        <li>
          <img 
            alt='<%= studyspace.name %>'
            src='<%= studyspace.image %>' 
          />
          <section
            class='item-container'
          >
            <p>
              <%= studyspace.name %>
            </p>
            <a 
              class='button-normal'
              href='/studyspaces/<%= studyspace._id %>'
            >
              See More
            </a>
          </section>
        </li>
        <% }); %>
      </ul>
    </section>
  </main>
  <script async defer src='/scripts/pages/studyspaces.js'></script>
</body>
</html>